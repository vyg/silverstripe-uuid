{
    "name": "madmatt/silverstripe-uuid",
    "description": "A module that allows for easy usage of UUIDs on Silverstripe DataObjects",
    "type": "silverstripe-vendormodule",
    "license": "BSD-3-Clause",
    "keywords": [
        "silverstripe",
        "cms",
        "uuid"
    ],
    "require": {
        "php": ">=8.1",
        "silverstripe/framework": "^5",
        "ramsey/uuid": "^4"
    },
    "require-dev": {
        "phpunit/phpunit": "^9",
        "squizlabs/php_codesniffer": "^3.5"
    },
    "autoload": {
        "psr-4": {
            "Madmatt\\UUID\\": "src/",
            "Madmatt\\UUID\\Tests\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "composer/installers": true,
            "silverstripe/vendor-plugin": true
        }
    }
}
